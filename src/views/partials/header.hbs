
<header class="navbar_">
  <div class="navbar__container">
    <nav class="navbar__nav" role="navigation">
      <ul class="navbar__list">
        <li class="navbar__item toggleBtn">
          <button class="toggleBTN"></button>
        </li>
        <li class="navbar__item">
          <a class="navbar__link {{#if activePage.home}}navbar__link--active{{/if}}" href="/">Home</a>
        </li>
         {{#unless activePage.signedIn}}
        <li class="navbar__item">
          <a class="navbar__link {{#if activePage.login}}navbar__link--active{{/if}}" href="/login">Login</a>
        </li>
        {{/unless}}
        {{#unless activePage.signedIn}}
        <li class="navbar__item">
          <a class="navbar__link {{#if activePage.register}}navbar__link--active{{/if}}" href="/register">Register</a>
        </li>
        {{/unless}}
        {{#if activePage.signedIn}}
        <li class="navbar__item">
          <a class="navbar__link {{#if activePage.register}}navbar__link--active{{/if}}" href="/logout">Logout</a>
        </li>
        {{/if}}

      </ul>
    </nav>
  </div>
</header>